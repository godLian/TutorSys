<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lhn.tutor.entity">
	<class name="Teacher" table="teacher">
		<id name="id" type="java.lang.String">
			<generator class="assigned"></generator>
		</id>
		<property name="name" type="java.lang.String"/>
		<property name="profile" type="java.lang.String"/>
		<property name="price" type="java.lang.String"/>
		<property name="tel" type="java.lang.String"/>
		<property name="firstteach" type="java.lang.String"/>
		<property name="secondteach" type="java.lang.String"/>
	</class>
</hibernate-mapping>