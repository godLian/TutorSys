<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lhn.tutor.entity">
	<class name="Teacher" table="teacher">
		<id name="id" type="java.lang.String">
			<generator class="assigned"></generator>
		</id>
		<property name="name" type="java.lang.String"/>
		<property name="idcard" type="java.lang.String"/>
		<property name="gender" type="java.lang.String"/>
		<property name="birthday" type="java.lang.String"/>
		<property name="place" type="java.lang.String"/>
		<property name="tel" type="java.lang.String"/>
		<property name="online" type="java.lang.String"/>
		<property name="email" type="java.lang.String"/>
		<property name="familyaddress" type="java.lang.String"/>
		<property name="zipcode" type="java.lang.String"/>
		<property name="user_id" type="int"/>
		<property name="identity" type="int"/>
		<property name="education" type="int"/>
		<property name="resume" type="int"/>
	</class>
</hibernate-mapping>